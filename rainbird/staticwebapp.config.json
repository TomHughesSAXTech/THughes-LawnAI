{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "allowedRoles": ["anonymous"]
    },
    {
      "route": "/*",
      "methods": ["GET"],
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/rainbird/rainbird-interface.html"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/rainbird/rainbird-interface.html"
    }
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".html": "text/html"
  }
}
